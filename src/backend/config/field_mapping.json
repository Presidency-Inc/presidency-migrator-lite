{
    "test_case_type_mapping": {
        "1": "Generic",
        "2": "Manual",
        "3": "Exploratory",
        "4": "Cucumber"
    },
    "priority_mapping": {
        "4": "Highest",
        "3": "High",
        "2": "Low",
        "1": "Lowest"
    },
    "automation_type_mapping": {
        "1": "Acceptance",
        "2": "Accessibility",
        "3": "Automated",
        "4": "Compatibility",
        "5": "Destructive",
        "6": "Functional",
        "7": "Other",
        "8": "Performance",
        "9": "Regression",
        "10": "Security",
        "11": "Smoke & Sanity",
        "12": "Usability"
    },
    "field_mapping": {
        "id": "__xray_testId",
        "title": "fields.summary",
        "type_id": "fields.issuetype.name",
        "section_id": {
            "type": "folder",
            "field": "xray_test_repository_folder",
            "use_hierarchy": true
        },
        "custom_steps": {
            "type": "manual_steps",
            "field": "fields.customfield_10100",
            "structure": {
                "action": "custom_steps",
                "data": "",
                "result": "custom_expected"
            }
        },
        "custom_steps_separated": {
            "type": "manual_steps",
            "field": "fields.customfield_10100",
            "structure": {
                "action": "content",
                "data": "additional_info",
                "result": "expected"
            }
        },
        "custom_testrail_bdd_scenario": {
            "type": "gherkin",
            "field": "fields.customfield_10101"
        },
        "custom_preconds": {
            "type": "precondition",
            "field": "fields.customfield_10102"
        },
        "priority_id": "fields.priority.id",
        "estimate": {
            "type": "timetracking",
            "field": "fields.timetracking"
        },
        "refs": "fields.labels",
        "custom_goals": "fields.description",
        "custom_mission": "fields.customfield_10103",
        "custom_automation_type": "fields.customfield_10104"
    },
    "section_mapping": {
        "depth_separator": "/",
        "root_folder": "TestRail"
    },
    "default_values": {
        "fields.project.key": "${JIRA_PROJECT_KEY}",
        "fields.issuetype.name": "Test"
    }
}